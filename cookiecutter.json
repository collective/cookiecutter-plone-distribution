{
  "distribution_title": "My Plone Distribution",
  "description": "A new Plone Distribution",
  "headless": [
    "1",
    "0"
  ],
  "author": "Plone Community",
  "email": "collective@plone.org",
  "github_organization": "collective",
  "python_package_name": "{{ cookiecutter.github_organization|lower }}.{{ cookiecutter.distribution_title|replace(' ', '')|replace('-', '_')|replace('.', '')|lower }}",
  "default_language": [
    "en",
    "de",
    "es",
    "pt-br",
    "nl",
    "fi"
  ],
  "include_features": [
    "0",
    "1"
  ],
  "__package_name": "{{ cookiecutter.python_package_name | package_name }}",
  "__python_package_name_upper": "{{ cookiecutter.python_package_name | pascal_case }}",
  "__package_namespace": "{{ cookiecutter.python_package_name | package_namespace }}",
  "__profile_version": "{% now 'utc', '%Y%m%d001' %}",
  "__generator_date_short": "{% now 'utc', '%Y-%m-%d' %}",
  "__generator_date_long": "{% now 'utc', '%Y-%m-%d %H:%M:%S' %}",
  "__generator_signature": "This was generated by [cookiecutter-plone-Distribution](https://github.com/collective/cookiecutter-plone-distribution) on {{ cookiecutter.__generator_date_long }}",
  "__docker_image_name" : "ghcr.io/{{ cookiecutter.github_organization }}/{{ cookiecutter.__package_name }}",
  "__prompts__": {
    "distribution_title": "Distribution Title",
    "description": "A short description of your distribution",
    "headless": {
      "__prompt__": "Is this a headless distribution?",
      "1": "Yes",
      "0": "No"
    },
    "github_organization": "GitHub Username or Organization",
    "python_package_name": "Python package name",
    "author": "Author",
    "email": "Author E-mail",
    "default_language": {
      "__prompt__": "Default Language",
      "en": "English",
      "de": "Deutsch",
      "es": "Español",
      "pt-br": "Português (Brasil)",
      "nl": "Nederlands",
      "fi": "Suomi"
    },
    "include_features": {
      "__prompt__": "Add example features (Profiles, Vocabularies)?",
      "1": "Yes",
      "0": "No"
    }
  },
  "_copy_without_render": [],
  "_extensions": [
    "local_extensions.pascal_case",
    "local_extensions.package_name",
    "local_extensions.package_namespace"
  ]
}
